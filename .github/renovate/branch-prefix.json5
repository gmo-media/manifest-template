{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  // Group by ./dev or ./prod
  additionalBranchPrefix: "{{#if (or (containsString packageFileDir 'dev/') (containsString packageFileDir 'prod/'))}}{{replace '(.*?)\\/.*' '$1' packageFileDir}}-{{else}}{{/if}}",
  commitMessagePrefix: "{{#if (or (containsString packageFileDir 'dev') (containsString packageFileDir 'prod'))}}[{{replace '(.*?)\\/.*' '$1' packageFileDir}}] {{else}}{{/if}}"
}

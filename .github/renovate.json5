{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    'docker:enableMajor',
    ':disableRateLimiting',
    ':dependencyDashboard',
    ':semanticCommits',
    ':automergeDigest',
    ':automergePatch',
    'helpers:pinGitHubActionDigests',
    'github>gmo-media/manifest-template//.github/renovate/auto-merge.json5',
    'github>gmo-media/manifest-template//.github/renovate/branch-prefix.json5',
    'github>gmo-media/manifest-template//.github/renovate/ignore.json5',
    'github>gmo-media/manifest-template//.github/renovate/labels.json5',
    'github>gmo-media/manifest-template//.github/renovate/minimum-release-age.json5',
    'github>gmo-media/manifest-template//.github/renovate/pin-digests.json5',
    'github>gmo-media/manifest-template//.github/renovate/regex-manager.json5',
  ],
  platform: 'github',
  platformAutomerge: true,
  automergeType: 'pr',
  onboarding: false,
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  suppressNotifications: [
    'prIgnoreNotification',
  ],
  branchConcurrentLimit: 0,
  prConcurrentLimit: 0,
  kubernetes: {
    managerFilePatterns: [
      '/.+\\.ya?ml$/',
    ],
  },
}
